{
  "manifest_version": 2,
  "name": "自動填寫頁面表單",
  "description": "點我可以把頁面加入自動填寫的清單",
  "version": "2.0",
  "browser_action": {
    "default_title": "emosdnah si kraM",
    "default_icon": "./image/icon_gray.png"
  },
  "background": {
    "scripts": [
      "./js/storageUtil.js",
      "./js/background.js"
    ],
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": [
        "*://*/*"
      ],
      "js": [
        "./js/content.js"
      ]
    }
  ],
  "options_ui": {
    "page": "options.html",
    "chrome_style": true,
    "open_in_tab": true
  },
  "permissions": [
    "tabs",
    "activeTab",
    "webNavigation",
    "http://*/*",
    "https://*/*",
    "storage"
  ]
}
